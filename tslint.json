{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "no-any": { "severity": "warn" },
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "prefer-const": true,
    "align": false,
    "import-name": false
  },
  "rulesDirectory": []
}
